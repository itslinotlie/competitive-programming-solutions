{ //binary exponentiation
	"C++ Template": {
        "prefix": "binpower",
        "body": [
		  "ll binpow(ll a, ll b) {",
		  "    ll res = 1;",
		  "    while(b>0) {",
		  "        if(b%2) res=(res*a)%mod;",
		  "        a=(a*a)%mod;",
		  "        b/=2;",
		  "    }",
		  "    return res;",
          "}"
        ],
        "description": ""
      }
